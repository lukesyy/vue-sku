{
    "name": "vue-sku-form",
    "description": "基于 Vue & ElementUI 的电商 SKU 表单配置组件",
    "keywords": [
        "sku",
        "vue",
        "element",
        "element-ui",
        "form"
    ],
    "version": "0.4.1",
    "directories": {
        "dist": "dist"
    },
    "homepage": "https://hooray.github.io/vue-sku-form",
    "private": false,
    "main": "./dist/SkuForm.umd.min.js",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "build-lib": "vue-cli-service build --target lib --name SkuForm ./src/components/index.js"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "element-ui": "^2.13.2",
        "vue": "^2.6.11",
        "vue-router": "^3.4.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.8.0",
        "eslint-plugin-vue": "^7.1.0",
        "sass": "^1.29.0",
        "sass-loader": "^10.0.1",
        "sass-resources-loader": "^2.1.0",
        "stylelint": "^13.7.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-scss": "^3.18.0",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
